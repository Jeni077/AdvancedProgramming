@model IEnumerable<AdvancedProgramming.Data.TaskHistory>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExecutionStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ExecutionEnd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Task.TaskName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExecutionStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExecutionEnd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Task.TaskName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.HistoryId }) |
            @Html.ActionLink("Details", "Details", new { id=item.HistoryId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.HistoryId })
        </td>
    </tr>
}

</table>
